{
   "license" : "MIT",
   "name" : "payever/sdk-php",
   "minimum-stability" : "stable",
   "homepage" : "https://github.com/payever/sdk-php",
   "require" : {
      "ext-curl" : "*",
      "psr/log" : "^1.1",
      "php" : ">=5.4.0",
      "ext-json" : "*"
   },
   "autoload" : {
      "classmap" : [
         "lib"
      ]
   },
   "keywords" : [
      "payments",
      "payever",
      "pay",
      "products",
      "inventory",
      "e-commerce"
   ],
   "type" : "library",
   "suggest" : {
      "ext-pdo" : "If you want to use MySQLLock",
      "ext-openssl" : "If you want to use openssl at PseudoRandomStringGenerator",
      "ext-mcrypt" : "If you want to use mcrypt at PseudoRandomStringGenerator"
   },
   "authors" : [
      {
         "role" : "Initial version",
         "homepage" : "https://getpayever.com/",
         "name" : "Andrey Puhovsky",
         "email" : "a.puhovsky@gmail.com"
      },
      {
         "email" : "gendosua@gmail.com",
         "name" : "Hennadii Shymanskyi",
         "role" : "1.x, 2.x and further development"
      }
   ],
   "autoload-dev" : {
      "psr-4" : {
         "Payever\\Tests\\" : "tests/"
      },
      "classmap" : [
         "lib",
         "tests"
      ]
   },
   "require-dev" : {
      "phpunit/phpunit" : "^6",
      "laminas/laminas-diactoros" : "^1.8",
      "squizlabs/php_codesniffer" : "^3.5",
      "friendsofphp/php-cs-fixer" : "^2.18",
      "behat/behat" : "^3.4",
      "phpmd/phpmd" : "^2.9"
   },
   "description" : "PHP SDK for payever"
}
